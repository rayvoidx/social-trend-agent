        }
      ]
    },
    {
      "title": "Workflow Metrics",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 49}
    },
    {
      "title": "Workflow Items by Status",
      "type": "piechart",
