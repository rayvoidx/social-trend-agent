{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "System Overview",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
    },
    {
      "title": "Active Jobs",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "targets": [
        {
          "expr": "sum(active_jobs)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 20, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "title": "Total API Requests (1h)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(api_requests_total[1h]))",
          "refId": "A"
        }
      ]
    },
    {
      "title": "LLM Requests (1h)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(llm_requests_total[1h]))",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
      "targets": [
        {
          "expr": "sum(rate(api_requests_total{status='error'}[5m])) / sum(rate(api_requests_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "thresholds": {
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "title": "LLM Metrics",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5}
    },
    {
      "title": "LLM Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {
          "expr": "sum(rate(llm_requests_total[5m])) by (provider, model)",
          "legendFormat": "{{provider}}/{{model}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "LLM Request Duration (p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le, provider, model))",
          "legendFormat": "{{provider}}/{{model}} p99",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Token Usage (1h)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
      "targets": [
        {
          "expr": "sum(increase(llm_tokens_total[1h])) by (provider, model, token_type)",
          "legendFormat": "{{provider}}/{{model}} {{token_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "LLM Cost (USD/hour)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
      "targets": [
        {
          "expr": "sum(increase(llm_cost_total[1h])) by (provider, model)",
          "legendFormat": "{{provider}}/{{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        }
      }
    },
    {
      "title": "Agent Metrics",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 22}
    },
    {
      "title": "Agent Run Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 23},
      "targets": [
        {
          "expr": "sum(rate(agent_runs_total[5m])) by (agent_name, status)",
          "legendFormat": "{{agent_name}} ({{status}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Agent Execution Duration (p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 23},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(agent_run_duration_seconds_bucket[5m])) by (le, agent_name))",
