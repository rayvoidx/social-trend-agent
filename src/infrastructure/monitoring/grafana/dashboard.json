      "targets": [
        {
          "expr": "sum(rate(vector_operations_total[5m])) by (operation, namespace)",
          "legendFormat": "{{operation}} ({{namespace}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Retrieval Results Count",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(retrieval_results_count_bucket[5m])) by (le, namespace))",
          "legendFormat": "{{namespace}} median",
