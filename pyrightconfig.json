{
  "include": ["src", "tests", "config"],
  "exclude": [".venv", "node_modules", "**/__pycache__", ".git"],
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": ["."]
    },
    {
      "root": "tests",
      "extraPaths": [".", "src"]
    }
  ]
}
